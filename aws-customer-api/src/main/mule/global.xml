	<api-gateway:autodiscovery apiId="19936061" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="7370ba86-98cc-4fd3-8389-236447bc955e" flowRef="aws-rest-api-main" />
	
</mule>
